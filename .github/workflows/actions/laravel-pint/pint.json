{
  "preset": "per",
  "rules": {
    "concat_space": {
      "spacing": "one"
    },
    "Laravel/laravel_phpdoc_alignment": true,
    "not_operator_with_successor_space": true,
    "no_unused_imports": true,
    "strict_param": true,
    "ordered_interfaces": true,
    "ordered_traits": true
  }
}
